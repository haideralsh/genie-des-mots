generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Gender {
  id       Int    @id
  gender   String
  comments Word[]
}

model Word {
  word                 String  @id
  gender_id            Int?
  has_silent_consonant Int?
  gender               Gender? @relation(fields: [gender_id], references: [id])
}
